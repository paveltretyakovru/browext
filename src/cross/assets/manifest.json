{
  "manifest_version": 2,
  "name": "Browext App. Dev",
  "description": "Creating repo for simple creating extensions",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "defaulr_title": "Browext App. Dev"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}